<view><view class="search-container"><image class="search" src="/static/icon/search.png"></image><input confirmType="search" maxlength="-1" placeholder="在此处搜索视频壁纸" data-event-opts="{{[['confirm',[['handleConfirm',['$event']]]],['input',[['inputChange',['$event']]]]]}}" value="{{keyword}}" bindconfirm="__e" bindinput="__e"/></view><scroll-view class="scroll-body scrollHG" throttle="{{false}}" enhanced="true" lowerThreshold="{{100}}" scroll-top="{{scrollTop}}" scroll-y="true" showScrollbar="false" data-event-opts="{{[['scrolltolower',[['onScrollToLower',['$event']]]],['scroll',[['scroll',['$event']]]]]}}" bindscrolltolower="__e" bindscroll="__e"><block tt:for="{{videos}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view class="grid-item" data-url="{{item.video_preview_url}}" data-event-opts="{{[['tap',[['preview',['$event']]]]]}}" bindtap="__e"><view class="g"><image class="photo" mode="aspectFill" src="{{item.thumb_img}}"></image><view class="info"><view class="count-container"><image class="iv" src="/static/images/play_num.png"></image><text class="count">{{item.play_num}}</text></view></view></view></view></block></scroll-view></view>