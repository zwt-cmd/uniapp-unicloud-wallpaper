
.u8_banner_wrap {
    position: relative;
    display: flex;
    align-items: center;
    height: 150rpx!important;
    padding: 0 20rpx;
    background: #fff!important;
    box-shadow: 0 0 7rpx 0 rgba(0,127,255,.5);
}
.u8_banner_logo_wrap {
    width: 100rpx;
    height: 100rpx;
