    margin-right: 30rpx;
}
.u8_banner_logo_wrap .image {
    width: 100%;
    height: 100%;
    border-radius: 50%;
    margin: 0 auto;
}
.u8_banner_desc_wrap {
    display: flex;
    flex-direction: column;
    width: 64%;
}
.u8_banner_name {
    color: #333!important;
    font-size: 30rpx!important;
    line-height: 42rpx!important;
}
.u8_banner_desc,.u8_banner_name {
    display: block!important;
    width: 100%!important;
