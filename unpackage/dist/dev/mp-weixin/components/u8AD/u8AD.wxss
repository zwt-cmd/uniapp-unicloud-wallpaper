.u8_banner_desc {
    color: #666!important;
    font-size: 22rpx!important;
    line-height: 34rpx!important;
}
.u8_banner_button {
    width: 110rpx!important;
    height: 52rpx!important;
    line-height: 52rpx!important;
    text-align: center;
    font-size: 22rpx!important;
    color: #06c;
    border: 1rpx solid #06c;
    border-radius: 27rpx;
}
.u8_banner_tip {
    position: absolute;
