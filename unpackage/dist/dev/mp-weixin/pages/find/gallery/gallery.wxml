<view><scroll-view class="bg-white nav" scroll-x="{{true}}"><view class="flex text-center"><block wx:for="{{categoryList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-item flex-sub '+(item.cid==TabCur?'text-cyan cur':'')]}}" data-cid="{{item.cid}}" data-event-opts="{{[['tap',[['tabSelect',['$event']]]]]}}" bindtap="__e">{{''+item.title+''}}</view></block></view></scroll-view><block wx:if="{{!loadFlag}}"><block><view class="{{['bottom',cssTran?'closeCss':'openCss']}}"><view class="nav-box"><view class="nav-tab"><u-icon vue-id="3db16520-5" name="/static/icon/back.png" size="{{44}}" data-event-opts="{{[['^__l',[['__l']]]]}}" bind:__l="__l"></u-icon><text>返回</text><button style="position:absolute;top:0;left:0;width:100%;height:100%;opacity:0;" open-type="navigateBack" data-event-opts="{{[['tap',[['fh',['$event']]]]]}}" bindtap="__e"></button></view><view class="nav-tab"><u-icon vue-id="3db16520-5" name="/static/icon/shares.png" size="{{44}}" data-event-opts="{{[['^__l',[['__l']]]]}}" bind:__l="__l"></u-icon><text>分享</text><button style="position:absolute;top:0;left:0;width:100%;height:100%;opacity:0;" open-type="share"></button></view></view></view></block></block><view class="cu-card dynamic"><block wx:for="{{photos}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cu-item shadow" style="height:260px;" data-item="{{item}}" data-event-opts="{{[['tap',[['info',['$event']]]]]}}" bindtap="__e"><view class="grid flex-sub col-1"><view class="bg-img only-img" style="{{('background-image:url('+item.card+');text-align:center')}}"><view class="cu-avatar round lg" style="{{('background-image:url('+item.avatar+');width:100px;height:100px;margin-top:100px')}}"></view><view class="content flex-sub"><view>{{item.name}}</view></view><view class="text-content">{{item.memo}}</view></view></view></view></block></view></view>